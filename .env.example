API_NAME="API Boilerplate"
API_DESCRIPTION="Super REST API Boilerplate with Go"
API_VERSION="0.0.1"
BASEURL="localhost:3000"
SERVER_PORT=3000

# Note: Swagger documentation for Audit & Access modules
# is controlled via build tags, not environment variables
# Use scripts/generate-swagger.sh --show-all to include them

DB_HOST=localhost
DB_PORT=5432
DB_USER=postgres
DB_PASSWORD=postgres
DB_NAME=your_database_name
DB_SSLMODE=disable

# This is useful if you want to hide specific endpoints so they do not appear in the documentation
# example: Example,Permission
API_DOC_FILTER=

# AI Configuration
# Base URL for AI API (OpenAI compatible)
AI_BASE_URL=https://api.openai.com/v1
# API Key for AI service
AI_API_KEY=your-openai-api-key-here
# Timeout in seconds for AI requests
AI_TIMEOUT=30

